pipeline {
    agent any

    stages {
        stage('Pull from GitHub') {
            steps {
                git 'https://github.com/Malaikakhattak69/Devops.git'
            }
        }

        stage('Build') {
            steps {
                bat 'echo Building the project...'
                // bat 'mvn package' or whatever your actual Windows build command is
            }
        }

        stage('Deploy') {
            steps {
                bat 'echo Deploying the project...'
                // add deployment steps here
            }
        }
    }
}
